<h1>Student Form</h1>

<%if @check==true %>

<%= @student.first_name %>
<%= @student.last_name %>
<%end%>

<%= form_tag students_path do %>
<label>Estudent Name: </label>
<%= text_field_tag :'first_name' %><br>
<label>Estudent Last Name: </label>
<%= text_field_tag :'last_name' %><br>
<%= submit_tag "Submit Student" %>
<% end %>
